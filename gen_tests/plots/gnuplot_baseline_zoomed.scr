filename=ARG1


set terminal pdfcairo font "Gill Sans,16" size 5in,3in linewidth 2 rounded #fontscale 1.0
set yrange [0:1]
set y2range [0:7000]
set xrange [3300:3600]
set ylabel "Utilization" offset 0
set y2label "Bandwidth (MB/s)" offset 0
set xlabel "Time (ms)" offset 0

set ytics nomirror
set y2tics

# options: lines, linespoints, boxes
# first column: time (s), second column: mc_all, third column: mc_cpu, fourth column: cpu0 bandwidth 
# show both refill and writeback

plot filename using ($2 / 16000) title "utilization" with lines axis x1y1,\
filename using (($4 * 64 * 1000000 + (1000*1024*1024 - 1)) / (1000*1024*1024)) title "bandwidth" with lines axis x1y2


#filename using (($3) / 16000) title "CPUutilization" with lines axis x1y1



#filename using (($2 - $3) / 16000) title "GPUutilization" with lines axis x1y1,\
